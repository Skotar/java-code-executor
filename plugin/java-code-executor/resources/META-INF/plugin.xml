<idea-plugin>
  <id>pl.beone.javacodeexecutor.plugin.JavaCodeExecutor</id>
  <name>Java code executor</name>
  <description>It allow you executing code on running Alfresco (you have to install java-code-executor module)</description>
  <version>1.0.0</version>
  <vendor>Bartosz Skotarek</vendor>

  <idea-version since-build="141.0"/>

  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="Alfresco" secondary="true" anchor="right"
                factoryClass="pl.beone.javacodeexecutor.plugin.JavaCodeExecutorToolWindowFactory"/>

  </extensions>

</idea-plugin>