<idea-plugin>
    <id>pl.beone.javacodeexecutor.plugin.JavaCodeExecutor</id>
    <name>Java code executor</name>
    <description>Executing code on running ACS (you have to install java-code-executor module)</description>
    <version>2.0.0</version>
    <vendor>Bartosz Skotarek</vendor>

    <idea-version since-build="141.0"/>

    <extensions defaultExtensionNs="com.intellij">
        <configurationType
                implementation="pl.beone.javacodeexecutor.intellij.plugin.internal.component.configuration.JavaCodeExecutorRunConfigurationType"/>
    </extensions>

</idea-plugin>